{
  "name": "shortlist-digital/agreable-promo-plugin",
  "type": "wordpress-plugin",
  "description": "Wordpress plugin for Shortlist Media.",
  "license": "MIT",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/getherbert/framework"
    },
    {
      "type": "package",
      "package": {
        "name": "jonsherrard/mesh",
        "version": "1.0",
        "dist": {
          "type": "zip",
          "url": "https://github.com/jonsherrard/mesh/archive/master.zip"
        }
      }
    }
  ],
  "require": {
    "jonsherrard/mesh": "*",
    "getherbert/framework": "~0.9",
    "facebook/webdriver": "~1.0",
    "behat/behat": "~2.5"
  },
  "config": {
      "bin-dir": "bin/"
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "AgreablePromoPlugin\\": "app/"
    }
  }
}
